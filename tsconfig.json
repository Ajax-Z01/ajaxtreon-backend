{
  "compilerOptions": {
  "target": "ES6",
  "module": "commonjs",
  "outDir": "./dist",
  "rootDir": "src",
  "strict": true,
  "esModuleInterop": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "baseUrl": "src",
  "paths": {
    "@shared/*": ["shared/*"]
  },
  "skipLibCheck": true
  },
  "include": [
  "**/*",
  "services/**/*",
  "shared/**/*"
  ],  
  "exclude": [
    "node_modules",
    "dist"
  ]
}
