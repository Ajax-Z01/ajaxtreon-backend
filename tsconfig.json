{
  "compilerOptions": {
  "target": "ES6",
  "module": "commonjs",
  "outDir": "./dist",
  "rootDir": "src",
  "strict": true,
  "esModuleInterop": true,
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "baseUrl": ".",
  "paths": {
    "@shared/*": ["src/shared/*"]
  },
  "skipLibCheck": true
  },
  "include": [
  "src/**/*",
  "src/services/**/*",
  "src/shared/**/*"
  ],  
  "exclude": [
    "node_modules",
    "dist"
  ]
}
